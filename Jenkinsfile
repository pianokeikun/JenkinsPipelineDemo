pipeline{
    agent any

    stages{
        stages('Hello'){
            steps{
                echo 'Hello World'
            }
        }stages('Build'){
            steps{
                echo 'Building'
            }
        }stages('Deploy'){
            steps{
                echo 'Deploying'
            }
        }stages('Test'){
            steps{
                echo 'Testing'
            }
        }stages('Release'){
            steps{
                echo 'Releasing'
            }
        }
    }
}
